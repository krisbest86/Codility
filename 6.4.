class Solution {    public int solution(int[] A) {        // write your code in C# 6.0 with .NET 4.5 (Mono)                //int[] B=new int[A.Length];        int min;        int max;        int suma=0;                for(int i=0;i<A.Length;i++){                        min=Math.Max(0,i-A[i]);            max=Math.Min(A.Length-1,i+A[i]);            Console.WriteLine("min: {0}, maks: {1}",min,max);                                                       }                        return suma;            }}

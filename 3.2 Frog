    class Solution5
    {
        public int solution(int X, int Y, int D)
        {
            // write your code in C# 6.0 with .NET 4.5 (Mono)

            if (Y > X)
            {
                double jumps = ((double)Y - (double)X) / (double)D;
                return (int)Math.Ceiling(jumps);

            }
            else
            {
                return 0;
            }

ã€€
        }
    }
